{
  "scripts": {
    "clearDist": "rimraf dist",
    "stylegen": "sass src/styles:src/styles",
    "prettier": "prettier --write",
    "lint": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\" && eslint --ext .ts,.tsx src --fix",
    "build": "npm run clearDist && npm run stylegen && webpack --config webpack.dev.cjs",
    "watchWP": "webpack --config webpack.dev.cjs --watch",
    "watchST": "sass --watch src/styles:src/styles",
    "dev": "npm run watchWP & npm run watchST"
  },
